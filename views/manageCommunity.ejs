        $('#UsersShowBtn').removeClass();
        $('#UsersShowBtn').addClass('manageCommunity-btn-active');
        $('#requestedUserShowBtn').removeClass();
        $('#requestedUserShowBtn').addClass('manageCommunity-btn');
