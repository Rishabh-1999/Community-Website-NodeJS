  <script type="text/javascript">
    var commid = "<%= data2._id %>"
    $(document).ready(function () {
      initaliseusers();
    })

    function initaliserequest() {
      var xml = new XMLHttpRequest();
      xml.open("POST", "/communityTable/getrequest");
      xml.setRequestHeader("Content-Type", "application/json");
      document.getElementById('comlist').innerHTML = "";
      xml.addEventListener("load", function () {
        $('#invitedUserShowBtn').removeClass()
        $('#invitedUserShowBtn').addClass('manageCommunity-btn');
        $('#managersShowBtn').removeClass();
        $('#managersShowBtn').addClass('manageCommunity-child-btn');
        $('#UsersShowBtn').removeClass();
        $('#UsersShowBtn').addClass('manageCommunity-child-btn');
        $('#requestedUserShowBtn').removeClass();
        $('#requestedUserShowBtn').addClass('manageCommunity-btn-active');
        var data = JSON.parse(xml.responseText);
        if (data.length == 0)
          noRequest();
        else {
          for (i in data)
            addToRequestDOM(data[i])
          document.getElementById('requestedUserShowBtn').innerHTML = "Requests (" + data.length + ")";
        }
        $('#requestedUserShowBtn').addClass('manageCommunity-btn-active');
      })
      xml.send(JSON.stringify({
        "_id": commid
      }));
    }

    function initaliseinvited() {
      var xml = new XMLHttpRequest();
      xml.open("POST", "/communityTable/getinveted");
      xml.setRequestHeader("Content-Type", "application/json");
      document.getElementById('comlist').innerHTML = ""
      xml.addEventListener("load", function () {
        var data = JSON.parse(xml.responseText);
        $('#invitedUserShowBtn').removeClass()
        $('#invitedUserShowBtn').addClass('manageCommunity-btn-active');
        $('#managersShowBtn').removeClass();
        $('#managersShowBtn').addClass('manageCommunity-child-btn');
        $('#UsersShowBtn').removeClass();
        $('#UsersShowBtn').addClass('manageCommunity-child-btn');
        $('#requestedUserShowBtn').removeClass();
        $('#requestedUserShowBtn').addClass('manageCommunity-child-btn');
        if (data.length == 0)
          noinvitedMembers();
        else {
          for (i in data)
            addToInvited(data[i])
          document.getElementById('invitedUserShowBtn').innerHTML = "Invited Members(" + data.length + ")";
        }
      })
      xml.send(JSON.stringify({
        "_id": commid
      }));
    }

    function addToRequestDOM(obj) {
      var div1 = document.createElement('div')
      div1.setAttribute("class", "col-sm-12 col-xs-12 allcoms community-user-div")
      div1.setAttribute("style", "margin-top:5px;display:flex;")
      var div2 = document.createElement('div')
      div2.setAttribute("class", "col-sm-2 col-xs-3")
      div2.setAttribute("style", "padding:5px;")
      var a1 = document.createElement('a')
      a1.setAttribute("href", "/communityTable/userprofile/" + obj._id)
      var img1 = document.createElement('img')
      img1.setAttribute("src", obj.photoloc)
      img1.setAttribute("class", "community-member-pic")
      a1.appendChild(img1)
      div2.appendChild(a1)
      div1.appendChild(div2)
      var div3 = document.createElement('div')
      div3.setAttribute("class", "col-sm-8 col-xs-6")
      var a2 = document.createElement('a')
      a2.setAttribute("class", "comusername")
      a2.innerHTML = obj.name
      a2.setAttribute("href", "/communityTable/userprofile/" + obj._id)
      div3.appendChild(a2)
      div1.appendChild(div3)
      var div4 = document.createElement('div')
      div4.setAttribute("class", "col-sm-2 col-xs-3")
      var a3 = document.createElement('a')
      a3.setAttribute("class", "community-user-short-btn")
      a3.setAttribute("style", "float:left")
      var i1 = document.createElement('i')
      i1.setAttribute("class", "fa fa-check")
      a3.appendChild(i1)
      a3.setAttribute("id", "requestacceptbtn")
      a3.setAttribute("onclick", "requestaccept('" + obj._id + "')");
      div4.appendChild(a3)
      var a4 = document.createElement('a')
      a4.setAttribute("class", "community-user-short-btn")
      a4.setAttribute("style", "float:right")
      a4.setAttribute("id", "requestrejectbtn")
      a4.setAttribute("onclick", "requestreject('" + obj._id + "')")
      var i2 = document.createElement('i')
      i2.setAttribute("class", "fa fa-times")
      a4.appendChild(i2)
      div4.appendChild(a4)
      div1.appendChild(div4)
      document.getElementById('comlist').appendChild(div1)
    }

    function addToInvited(obj) {
      var div1 = document.createElement('div')
      div1.setAttribute("class", "col-sm-12 col-xs-12 allcoms community-user-div")
      div1.setAttribute("style", "margin-top:5px;display:flex;")
      var div2 = document.createElement('div')
      div2.setAttribute("class", "col-sm-2 col-xs-3")
      div2.setAttribute("style", "padding:5px;")
      var a1 = document.createElement('a')
      a1.setAttribute("href", "/communityTable/userprofile/" + obj._id)
      var img1 = document.createElement('img')
      img1.setAttribute("src", obj.photoloc)
