}

.fa {
	color: #fff;
	font-size: 25px;
	width: 30px;
	margin: 6px 15px;
	margin-left: 11px;
	box-sizing: border-box;
}

#menu span {
	display: none;
}

.menu-item {
	cursor: pointer;
	opacity: 0.8;
	color: #fff;
	font-family: sans-serif;
	font-size: 14px;
}

.raj {
