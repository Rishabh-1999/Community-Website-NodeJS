	opacity: 1;
	background-color: #4d5866;
}

.active {
	background-color: #51a1e2 !important;
}

@media (max-width: 768px) {

.none-mobile-view-dp {
	visibility: hidden;
    
}

.mobile {
	visibility: visible;
}

}
</style>
<body>

	 <nav class="navbar navbar-default-bar no-margin navbar-bar-head-fixed bg-dark"  id="nav-header">

	 	<div class="navbar-header fixed-brand">
      
            <span class="label label-success mobile" style="margin-top:20px;float:right;visibility: hidden; color: white; "id="admin_display">Admin</span>

            <i class="fa fa-bars" class="menu-item" id="sideBtn" style="margin-top: 12px;"></i>
        
            <a class="navbar-brand" href="#" style="color: white;font-weight: bold;">cq</a>
     	</div>

     	<div class="label-success mobile">
     		
     		<img src="images/logo.png" class="profilePic">
     		<span class="label label-success" style="float:right;margin-top:17px;margin-left:5px;color: white;">Admin</span>
     	</div>

